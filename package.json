{
  "name": "micro-systems-config",
  "description": "Returns config for the systems",
  "version": "1.1.5",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "NODE_ENV=production micro -p 8080",
    "dev": "micro -p 3000",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "config",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/micro-systems-config.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/micro-systems-config/issues"
  },
  "now": {
    "alias": "systems.config.tfk.allthethings.win"
  },
  "greenkeeper": {
    "emails": false
  },
  "devDependencies": {
    "coveralls": "2.11.16",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "10.0.2"
  },
  "dependencies": {
    "micro": "7.0.2"
  }
}
